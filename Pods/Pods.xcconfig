FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/Firebase"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/Firebase" "${PODS_ROOT}/Headers/FlatUIKit"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/Firebase" -isystem "${PODS_ROOT}/Headers/FlatUIKit"
OTHER_LDFLAGS = -ObjC -lc++ -licucore -framework CFNetwork -framework CoreText -framework Firebase -framework Security -framework SystemConfiguration
PODS_ROOT = ${SRCROOT}/Pods